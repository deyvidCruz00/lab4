{
  
    "id": null,
    "title": "Nginx Dashboard",
    "tags": ["nginx", "load-balancer"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Nginx Active Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "nginx_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Nginx Connection Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(nginx_connections_accepted[5m])",
            "legendFormat": "Accepted Connections/sec"
          },
          {
            "expr": "rate(nginx_connections_handled[5m])",
            "legendFormat": "Handled Connections/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "HTTP Requests Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(nginx_http_requests_total[5m])",
            "legendFormat": "HTTP Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Connection States",
        "type": "timeseries",
        "targets": [
          {
            "expr": "nginx_connections_reading",
            "legendFormat": "Reading"
          },
          {
            "expr": "nginx_connections_writing",
            "legendFormat": "Writing"
          },
          {
            "expr": "nginx_connections_waiting",
            "legendFormat": "Waiting"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  
}